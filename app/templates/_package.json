{
  "name": "<%= _.slugify(moduleName) %>",
  "description": "<%= moduleDescription %>",
  "version": "0.0.0",
  "dependencies": {},
  "scripts": {
    "lint": "eslint -f unix.",
    "pretest": "npm run lint",
    "test": "istanbul cover ./node_modules/.bin/_mocha test",
    "posttest": "istanbul check-coverage --statements 85 --branches 70 --functions 100 --lines 90 && rm -rf coverage"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "<%= fullName %> <<%= emailAddress %>>",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^1.4.1",
    "eslint-config-clock": "^1.0.0",
    "eslint-config-standard": "^4.3.2",
    "eslint-plugin-standard": "^1.3.0",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:<%= authorName %>/<%= _.slugify(moduleName) %>.git"
  }
}
